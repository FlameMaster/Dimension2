<?xml version="1.0" encoding="utf-8"?>

<!--
*  布局说明：
*  所属分类：
-->
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/icon_end" android:drawable="@drawable/ic_main_tab_media_h" android:state_checked="true"/>
    <item android:id="@+id/icon_start" android:drawable="@drawable/ic_main_tab_media"/>
    <transition
        android:fromId="@id/icon_start"
        android:toId="@id/icon_end">
        <animated-vector android:drawable="@drawable/ic_main_tab_media_h">
            <target
                android:animation="@animator/objanim_main_tab"
                android:name="group"
                />
        </animated-vector>
    </transition>

</animated-selector>