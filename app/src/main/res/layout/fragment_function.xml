<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F0F0F0"
    android:overScrollMode="never"
    android:scrollbars="none"
    tools:context="com.melvinhou.dimension2.home.FunctionFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingBottom="72dp"
        >
        <include
            android:id="@+id/in_fun_document"
            layout="@layout/layout_fun_document"/>
        <include
            android:id="@+id/in_fun_sys"
            layout="@layout/layout_fun_sys"/>
        <include
            android:id="@+id/in_fun_im"
            layout="@layout/layout_fun_im"/>
        <include
            android:id="@+id/in_fun_vr"
            layout="@layout/layout_fun_vr"/>
        <include
            android:id="@+id/in_fun_media"
            layout="@layout/layout_fun_media"/>


    </LinearLayout>
</androidx.core.widget.NestedScrollView>