<?xml version="1.0" encoding="utf-8"?>

<!--
*  布局说明：
*  所属分类：
-->
<androidx.constraintlayout.widget.ConstraintLayout
    android:id="@+id/root"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/bg_box"
    android:layout_marginHorizontal="30dp"
    >

    <TextView
        app:layout_constraintHorizontal_chainStyle="spread_inside"
        android:id="@+id/tv_01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_box"
        android:paddingHorizontal="16dp"
        android:paddingVertical="8dp"
        android:text="A01"
        android:textColor="@color/black"
        app:layout_constraintEnd_toStartOf="@+id/tv_02"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_box"
        android:paddingHorizontal="16dp"
        android:paddingVertical="8dp"
        android:text="B01"
        android:textColor="@color/black"
        app:layout_constraintEnd_toStartOf="@+id/tv_03"
        app:layout_constraintStart_toEndOf="@+id/tv_01"
        app:layout_constraintTop_toTopOf="@id/tv_01" />

    <TextView
        android:id="@+id/tv_03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_box"
        android:paddingHorizontal="16dp"
        android:paddingVertical="8dp"
        android:text="C01"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_02"
        app:layout_constraintTop_toTopOf="@id/tv_01" />

    <TextView
        app:layout_constraintHorizontal_chainStyle="spread"
        android:id="@+id/tv_04"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_box"
        android:paddingHorizontal="16dp"
        android:paddingVertical="8dp"
        android:text="A02"
        android:textColor="@color/black"
        app:layout_constraintEnd_toStartOf="@+id/tv_05"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_01" />

    <TextView
        android:id="@+id/tv_05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_box"
        android:paddingHorizontal="16dp"
        android:paddingVertical="8dp"
        android:text="B02"
        android:textColor="@color/black"
        app:layout_constraintEnd_toStartOf="@+id/tv_06"
        app:layout_constraintStart_toEndOf="@+id/tv_04"
        app:layout_constraintTop_toTopOf="@id/tv_04" />

    <TextView
        android:id="@+id/tv_06"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_box"
        android:paddingHorizontal="16dp"
        android:paddingVertical="8dp"
        android:text="C02"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_05"
        app:layout_constraintTop_toTopOf="@id/tv_04" />

    <TextView
        app:layout_constraintHorizontal_chainStyle="packed"
        android:id="@+id/tv_07"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_box"
        android:paddingHorizontal="16dp"
        android:paddingVertical="8dp"
        android:text="A03"
        android:textColor="@color/black"
        app:layout_constraintEnd_toStartOf="@+id/tv_08"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_04" />

    <TextView
        android:id="@+id/tv_08"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_box"
        android:paddingHorizontal="16dp"
        android:paddingVertical="8dp"
        android:text="B03"
        android:textColor="@color/black"
        app:layout_constraintEnd_toStartOf="@+id/tv_09"
        app:layout_constraintStart_toEndOf="@+id/tv_07"
        app:layout_constraintTop_toTopOf="@id/tv_07" />

    <TextView
        android:id="@+id/tv_09"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_box"
        android:paddingHorizontal="16dp"
        android:paddingVertical="8dp"
        android:text="C03"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_08"
        app:layout_constraintTop_toTopOf="@id/tv_07" />

    <TextView
        app:layout_constraintHorizontal_weight="2"
        android:id="@+id/tv_10"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_box"
        android:paddingHorizontal="16dp"
        android:paddingVertical="8dp"
        android:text="A04"
        android:textColor="@color/black"
        app:layout_constraintEnd_toStartOf="@+id/tv_11"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_07" />

    <TextView
        app:layout_constraintHorizontal_weight="3"
        android:id="@+id/tv_11"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_box"
        android:paddingHorizontal="16dp"
        android:paddingVertical="8dp"
        android:text="B04"
        android:textColor="@color/black"
        app:layout_constraintEnd_toStartOf="@+id/tv_12"
        app:layout_constraintStart_toEndOf="@+id/tv_10"
        app:layout_constraintTop_toTopOf="@id/tv_10" />

    <TextView
        app:layout_constraintHorizontal_weight="4"
        android:id="@+id/tv_12"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_box"
        android:paddingHorizontal="16dp"
        android:paddingVertical="8dp"
        android:text="C04"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_11"
        app:layout_constraintTop_toTopOf="@id/tv_10" />

    <TextView
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintHorizontal_bias="0.3"
        android:id="@+id/tv_13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_box"
        android:paddingHorizontal="16dp"
        android:paddingVertical="8dp"
        android:text="A05"
        android:textColor="@color/black"
        app:layout_constraintEnd_toStartOf="@+id/tv_14"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_10" />

    <TextView
        android:id="@+id/tv_14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_box"
        android:paddingHorizontal="16dp"
        android:paddingVertical="8dp"
        android:text="B05"
        android:textColor="@color/black"
        app:layout_constraintEnd_toStartOf="@+id/tv_15"
        app:layout_constraintStart_toEndOf="@+id/tv_13"
        app:layout_constraintTop_toTopOf="@id/tv_13" />

    <TextView
        android:id="@+id/tv_15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_box"
        android:paddingHorizontal="16dp"
        android:paddingVertical="8dp"
        android:text="C05"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_14"
        app:layout_constraintTop_toTopOf="@id/tv_13" />



</androidx.constraintlayout.widget.ConstraintLayout>
